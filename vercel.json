{
  "rewrites": [
    {
      "source": "/:path*",
      "has": [
        { "type": "host", "value": "api.deepwoken.app" }
      ],
      "destination": "/api/:path*"
    },
    {
      "source": "/:path*",
      "has": [
        { "type": "host", "value": "deepwoken.app" }
      ],
      "destination": "/:path*.html"
    }
  ]
}